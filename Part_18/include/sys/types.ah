#ifndef SYS_TYPES_AH
# define SYS_TYPES_AH

// Some simpler type names for the x64 platform
type char = int8;
type int  = int32;
type uint = uint32;

type time_t = int64;
type dev_t = uint64;
type ino_t = uint64;
type nlink_t = uint64;
type mode_t = uint32;
type uid_t = uint32;
type gid_t = uint32;
type off_t = int64;
type blksize_t = int64;
type blkcnt_t = int64;

#endif
